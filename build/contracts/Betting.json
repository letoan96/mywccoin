  "source": "contract Betting {\n\n    bool isOn;\n    string public teamA;\n    string public teamB;\n    string public description;\n\n    struct Bet {\n      address player;\n      uint256 amount;\n    }\n\n    function Betting(string _teamA, string _teamB, string _description) {\n      teamA = _teamA;\n      teamB = _teamB;\n      description = _description;\n    }\n\n}\n",
